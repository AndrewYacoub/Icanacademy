<style>
        .carousel-item img {
            height: 650px; /* Adjust this value as needed */
            object-fit: cover;
        }
        .carousel-item {
            height: 650px; /* Adjust this value as needed */
        }
        .owl-carousel .owl-item img {
            height: 500px; /* Adjust based on your design needs */
            width: 100%;
            object-fit: cover; /* This ensures images cover the area without distorting aspect ratio */
        }

        .owl-carousel .owl-item {
            height: 500px; /* This height should be enough to accommodate your text and other content without changes */
        }
        .carousel .carousel-item img {
            filter: brightness(50%); /* Lower percentage = darker image */
        }
        .service-card {
            transition: transform 0.3s ease, filter 0.3s ease, background-color 0.3s ease, color 0.3s ease; /* Ensure smooth transition for background and color changes */
            color: black; /* Default text color */
            background-color: white; /* Default background color */
        }

        .service-card:hover {
            transform: scale(1.05);
            filter: brightness(1.15);
            background-color: #007bff; /* Blue background on hover */
            color: white; /* White text on hover */
        }

        .service-card .card-body i, .service-card .card-body h5, .service-card .card-body p {
            color: inherit; /* Icons, headers, and text inherit color from the card */
        }
        .card-body i {
            color: #1753fc;
        }
        .service-card:hover i {
            color: white; /* Specifically make icons white on hover */
        }
        .section-title {
            position: relative;
            display: inline-block;
            text-transform: uppercase;
        }
        .hover-effect:hover {
            transform: translateY(-5px);
            transition: transform .3s ease-in-out;
        }

        .social-icons a {
            transition: all .3s ease-in-out;
        }

        .social-icons a:hover {
            background-color: #007bff; /* Primary color */
            color: white;
        }
        .hover-effect:hover {
            transform: scale(1.05);
            transition: transform .3s ease-in-out, box-shadow .3s ease-in-out;
            box-shadow: 0 8px 25px -8px rgba(0, 0, 0, 0.1);
        }

        .testimonial-item:hover {
            transform: scale(1.03); /* Slight scale on hover */
            box-shadow: 0 8px 16px rgba(0,0,0,0.1); /* Enhanced shadow for better visibility */
        }

        .carousel-item {
            transition: transform 0.5s ease; /* Smooth transition for carousel items */
        }
    </style>
